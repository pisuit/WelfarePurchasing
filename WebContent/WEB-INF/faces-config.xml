   <to-view-id>/purchasinggi.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>MENU_HELP_MANUAL</from-outcome>
   <to-view-id>/manual.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>MENU_STOCK_MINIMUM</from-outcome>
   <to-view-id>/stockminimum.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>MENU_LOG</from-outcome>
   <to-view-id>/logadmin.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>MENU_STOCK_ADMIN</from-outcome>
   <to-view-id>/stockadmin.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <message-bundle>welfare.resources.messages_TH</message-bundle>
 </application>
