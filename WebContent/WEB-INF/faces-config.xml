 <navigation-rule>
  <from-view-id>/login.jsp</from-view-id>
  <navigation-case>
   <from-action>#{login.login}</from-action>
   <from-outcome>SUCCESS</from-outcome>
   <to-view-id>/welcome.jsp</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-action>#{login.logout}</from-action>
   <from-outcome>LOGOUT</from-outcome>
   <to-view-id>/login.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>LOGIN</from-outcome>
   <to-view-id>/login.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>MENU_HOME</from-outcome>
   <to-view-id>/welcome.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>MENU_BUDGET_MAIN</from-outcome>
   <to-view-id>/budgetmain.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>MENU_BUDGET_DETAIL</from-outcome>
   <to-view-id>/budgetdetail.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>MENU_BUDGET_TRANSFER_REQUEST</from-outcome>
   <to-view-id>/budgettransferrequest.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>MENU_BUDGET_TRANSFER_APPROVE</from-outcome>
   <to-view-id>/budgettransferapprove.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>MENU_BUDGET_EXPENSE</from-outcome>
   <to-view-id>/budgetexpense.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>MENU_BUDGET_ADMIN</from-outcome>
   <to-view-id>/budgetadmin.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>MENU_PURCHASING_ADMIN</from-outcome>
   <to-view-id>/purchasingadmin.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>MENU_STOCK_MATGROUP</from-outcome>
   <to-view-id>/stockmatgroup.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>MENU_STOCK_MAT</from-outcome>
   <to-view-id>/stockmat.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>MENU_PURCHASING_VENDOR</from-outcome>
   <to-view-id>/purchasingvendor.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-action>#{menu.pr}</from-action>
   <from-outcome>MENU_PURCHASING_PR</from-outcome>
   <to-view-id>/purchasingpr.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-action>#{menu.po}</from-action>
   <from-outcome>MENU_PURCHASING_PO</from-outcome>
   <to-view-id>/purchasingpo.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-action>#{menu.gr}</from-action>
   <from-outcome>MENU_PURCHASING_GR</from-outcome>
   <to-view-id>/purchasinggr.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
